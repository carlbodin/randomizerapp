<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/dice350"
        android:label="Randomizer"
        android:roundIcon="@drawable/dice350"
        android:supportsRtl="true"
        android:theme="@style/Theme.Randomizer"
        tools:targetApi="31">
        <activity
            android:name=".CategoriesSetupActivity"
            android:exported="false" />
        <activity
            android:name=".CategoriesActivity"
            android:exported="false" />
        <activity
            android:name=".CoinActivity"
            android:exported="false" />
        <activity
            android:name=".DiceActivity"
            android:exported="false" />
        <activity
            android:name=".YatzyActivity"
            android:exported="false" />
        <activity
            android:name=".DiceDynamicActivity"
            android:exported="false" />
        <activity
            android:name=".StartMenu"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>